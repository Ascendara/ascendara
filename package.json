{
  "name": "ascendara",
  "version": "1.0.0",
  "main": "main.js",
  "description": "",
  "scripts": {
    "dev": "vite --mode development ./src",
    "build": "vite build ./src",
    "build-react": "npm run build && electron-builder",
    "electron": "electron ./build/main.js",
    "start": "concurrently \"npm run dev\" \"npm run electron\"",
    "dist": "electron-builder -c.extraMetadata.main=build/main.js"
  },
  "dependencies": {
    "@million/lint": "^1.0.0-rc.13",
    "@nextui-org/react": "latest",
    "axios": "^1.6.8",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "electron-dl": "^4.0.0",
    "framer-motion": "^11.2.6",
    "million": "^3.1.6",
    "path": "^0.12.7",
    "react": "latest",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "todesktop": "^0.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "^10.4.14",
    "brfs": "^2.0.2",
    "concurrently": "^7.6.0",
    "electron": "^30.0.7",
    "electron-builder": "^24.13.3",
    "esbuild-wasm": "0.17.12",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "vite": "^4.1.4"
  },
  "keywords": [],
  "author": "tagoWorks",
  "icon": "ascendara.ico",
  "build": {
    "appId": "com.tagoworks.ascendara",
    "files": [
      "build/*",
      "node_modules/**/*",
      "package.json"
    ],
    "productName": "Ascendara",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
