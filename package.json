{
  "name": "ascendara",
  "version": "7.7.0",
  "main": "main.js",
  "description": "Skip extracting files, navigating setup processes, or any unnecessary hassles.",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build",
    "build-react": "npm run build && electron-builder",
    "electron": "electron ./build/main.js",
    "start": "concurrently \"vite\" \"wait-on tcp:5173 && electron ./build/main.js\"",
    "dist": "python ./scripts/build_ascendara.py",
    "buildwithelectron": "electron-builder -c.extraMetadata.main=build/main.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@million/lint": "^1.0.14",
    "@nextui-org/react": "latest",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@tanstack/react-virtual": "^3.11.2",
    "@trigger.dev/sdk": "^3.3.7",
    "0r": "^1.0.0",
    "axios": "^1.6.8",
    "babel-plugin-react-compiler": "^19.0.0-beta-27714ef-20250124",
    "child_process": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "discord-rpc": "^4.0.1",
    "diskusage": "^1.2.0",
    "dotenv": "^16.4.5",
    "electron-dl": "^4.0.0",
    "framer-motion": "^11.13.3",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.0.0",
    "i18next": "^24.1.2",
    "ldrs": "^1.0.2",
    "lockfile": "^1.0.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.468.0",
    "next-themes": "^0.4.4",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.2.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^6.23.1",
    "react-window": "^1.8.11",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "unzipper": "^0.12.1",
    "winreg": "^1.2.5"
  },
  "devDependencies": {
    "@trigger.dev/build": "^3.3.7",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "^10.4.14",
    "brfs": "^2.0.2",
    "concurrently": "^7.6.0",
    "electron": "^30.1.0",
    "electron-builder": "^24.13.3",
    "esbuild-wasm": "0.17.12",
    "html-to-image": "^1.11.11",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.21",
    "prettier": "^3.4.2",
    "prettier-plugin-sort-json": "4.1.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.16",
    "vite": "^4.1.4",
    "wait-on": "^8.0.1"
  },
  "keywords": [],
  "author": "tagoWorks",
  "icon": "build/icon.ico",
  "build": {
    "asar": true,
    "appId": "com.tagoworks.ascendara",
    "icon": "build/icon.ico",
    "files": [
      "build/*",
      "package.json",
      "public/*",
      ".env"
    ],
    "extraResources": [
      {
        "from": "public",
        "to": "public"
      },
      {
        "from": "binaries/AscendaraDownloader/dist/AscendaraDownloader.exe",
        "to": "."
      },
      {
        "from": "binaries/AscendaraDownloader/dist/AscendaraGofileHelper.exe",
        "to": "."
      },
      {
        "from": "binaries/AscendaraGameHandler/dist/AscendaraGameHandler.exe",
        "to": "."
      },
      {
        "from": "binaries/AscendaraCrashReporter/dist/AscendaraCrashReporter.exe",
        "to": "."
      }
    ],
    "extraMetadata": {
      "main": "build/main.js"
    },
    "win": {
      "target": "nsis"
    },
    "productName": "Ascendara",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md,html,css}": [
      "prettier --write"
    ]
  }
}
