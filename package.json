{
  "name": "ascendara",
  "version": "10.2.1",
  "main": "electron/app.js",
  "description": "Skip extracting files, navigating setup processes, or any unnecessary hassles.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ascendara/ascendara.git",
    "baseUrl": "https://github.com/Ascendara/ascendara"
  },
  "license": "CC-BY-NC-1.0",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "author": "tagoWorks",
  "keywords": [
    "electron",
    "vite",
    "setup",
    "installer",
    "automation",
    "onboarding",
    "user-experience",
    "cross-platform",
    "desktop-app",
    "productivity",
    "hassle-free",
    "quick-start",
    "packaging",
    "software-launcher",
    "configuration"
  ],
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build",
    "build-react": "vite build && electron-builder --config.extraMetadata.main=electron/app.js",
    "electron": "electron ./electron/app.js --no-sandbox",
    "start": "concurrently \"vite\" \"wait-on tcp:5173 && electron ./electron/app.js --no-sandbox\"",
    "dist": "python3 ./scripts/build_ascendara.py",
    "dist-linux": "python3 ./scripts/build_ascendara_linux.py",
    "buildwithelectron": "electron-builder --config.extraMetadata.main=electron/app.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
