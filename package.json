{
  "name": "ascendara",
  "version": "4.1.1",
  "main": "main.js",
  "description": "Effortlessly install games directly onto your PC without the hassle of extractions or file locations.",
  "scripts": {
    "dev": "vite --mode development ./src",
    "build": "vite build ./src",
    "build-react": "npm run build && electron-builder",
    "electron": "electron ./build/main.js",
    "start": "concurrently \"npm run dev\" \"npm run electron\"",
    "distenv": "electron-builder -c.extraMetadata.main=build/main-build.js",
    "dist": "electron-builder -c.extraMetadata.main=build/main.js"
  },
  "dependencies": {
    "@nextui-org/react": "latest",
    "axios": "^1.6.8",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "electron-dl": "^4.0.0",
    "framer-motion": "^11.2.6",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.0.0",
    "path": "^0.12.7",
    "react": "latest",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "^10.4.14",
    "brfs": "^2.0.2",
    "concurrently": "^7.6.0",
    "electron": "^30.1.0",
    "electron-builder": "^24.13.3",
    "esbuild-wasm": "0.17.12",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "vite": "^4.1.4"
  },
  "keywords": [],
  "author": "tagoWorks",
  "icon": "build/icon.ico",
  "build": {
    "asar": true,
    "appId": "com.tagoworks.ascendara",
    "extraResources": [
      {
        "from": "binaries/AscendaraDownloader/dist/AscendaraDownloader.exe",
        "to": "."
      },
      {
        "from": "binaries/AscendaraGameHandler/dist/AscendaraGameHandler.exe",
        "to": "."
      }
    ],
    "files": [
      "build/*",
      "package.json"
    ],
    "productName": "Ascendara",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
